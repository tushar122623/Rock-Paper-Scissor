#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/usr/bin.env" ]; then
  exec "$basedir/usr/bin.env" node "$basedir/../rock-paper/main.js" "$@"
else 
  exec usr/bin.env node "$basedir/../rock-paper/main.js" "$@"
fi
